<?xml version="1.0" encoding="UTF-8"?>

<PictureUploadData xmlns="http://www.isapi.org/ver20/XMLSchema" version="2.0">
  <!--opt, object, 图片信息, attr:version{req, string, 协议版本}-->
  <FDID>
    <!--req, string, 人脸比对库唯一标示ID-->test
  </FDID>
  <FaceAppendData>
    <!--opt, object, 人脸附加信息-->
    <bornTime>
      <!--opt, string, 出生日期-->test
    </bornTime>
    <name>
      <!--req, string, 姓名, range:[1,32]-->test
    </name>
    <sex>
      <!--opt, enum, 性别, subType:string, [male#男,female#女]-->male
    </sex>
    <province>
      <!--opt, string, 省份, desc:参考《全国各省份城市列表.txt》-->11
    </province>
    <city>
      <!--opt, string, 城市, desc:参考《全国各省份城市列表.txt》-->11
    </city>
    <certificateType>
      <!--opt, enum, 证件类型, subType:string, [officerID#军官证,ID#身份证,passportID#护照,other#其他]-->officerID
    </certificateType>
    <certificateNumber>
      <!--opt, string, 证件号, range:[8,32], desc:当certificateType为officerID时，certificateNumber长度范围[8,18]；当certificateType为ID时，certificateNumber长度范围[8,18]；当certificateType为passportID时，certificateNumber长度范围[8,32]；当certificateType为other时，certificateNumber长度范围[8,18]；-->test
    </certificateNumber>
    <PersonInfoExtendList>
      <!--opt, array, 人员自定义扩展信息, subType:object-->
      <PersonInfoExtend>
        <!--opt, object, 人员信息-->
        <id>
          <!--req, int, 人员扩展信息序号, desc:从1开始赋值,依次增加-->1
        </id>
        <enable>
          <!--req, bool, 人员扩展信息使能-->true
        </enable>
        <name>
          <!--opt, string, 人员标签信息扩展名称-->test
        </name>
        <value>
          <!--opt, string, 人员标签信息扩展内容-->test
        </value>
      </PersonInfoExtend>
    </PersonInfoExtendList>
    <caseInfo>
      <!--opt, string, 案件信息, range:[1,64]-->test
    </caseInfo>
    <customHumanID>
      <!--opt, string, 自定义人员ID, range:[1,64]-->test
    </customHumanID>
    <phoneNumber>
      <!--opt, string, 电话号码, range:[1,64]-->test
    </phoneNumber>
    <modeldata>
      <!--opt, string, 建模数据, desc:使用base64加密,base64导入方式-->test
    </modeldata>
    <userType>
      <!--opt, enum, 人员类型, subType:string, [normal#普通人,visitor#访客]-->normal
    </userType>
    <associatedPlateType>
      <!--opt, enum, 关联车牌类型, subType:string, [greenChannel#绿色通道车牌,normal#普通], desc:默认不区分关联车牌类型,适用于安检人车核验一体机产品进行人车核验时使用-->greenChannel
    </associatedPlateType>
  </FaceAppendData>
  <picURL>
    <!--opt, string, 图片URL-->test
  </picURL>
</PictureUploadData>
